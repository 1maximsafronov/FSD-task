{
  "name": "FSD-task",
  "version": "1.0.0",
  "private": true,
  "description": "Тестовое задание в рамках обучения от Fullstack Development",
  "license": "MIT",
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "src/*.html",
    "src/js/**/*.js",
    "src/img/**/*.svg",
    "src/sass/**/*.{sass,scss}"
  ],
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "eslint": "eslint src/**/*.js",
    "stylelint": "stylelint \"src/sass/**/*.scss\" --syntax scss",
    "test": "npm run editorconfig && npm run stylelint && npm run eslint",
    "start": "webpack-dev-server  --open --mode development",
    "build": "webpack --mode production"
  },
  "dependencies": {
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "eslint": "6.1.0",
    "eslint-config-htmlacademy": "0.5.2",
    "editorconfig": "^0.15.3",
    "@htmlacademy/editorconfig-cli": "^1.0.0",
    "stylelint": "^13.3.3",
    "stylelint-config-htmlacademy": "^0.1.4",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "autoprefixer": "^9.7.6",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.3",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "pug": "^2.0.4",
    "pug-loader": "^2.4.0",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.42.1",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "engines": {
    "node": "10"
  }
}
